<Window x:Class="ProToExlForBD.CreOtcToExl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProToExlForBD"
        mc:Ignorable="d"
        Title="CreOtcToExl" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="DT" HorizontalAlignment="Left" Height="291" Margin="44,49,0,0" VerticalAlignment="Top" Width="706">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Name" Binding="{Binding Name}" />
                <DataGridTemplateColumn>
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <Button x:Name="EditBtn" Content="Создать отчет" Width="auto" Click="EditBtn_Click_1"/>
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTextColumn />
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Создать отчет" HorizontalAlignment="Left" VerticalAlignment="Top" Width="772" Margin="10,388,0,0"/>
        <Label FontSize="18" Content="Выбеите необходимый продукт" HorizontalAlignment="Left" Margin="253,10,0,0" VerticalAlignment="Top" Width="282"/>

    </Grid>
</Window>
